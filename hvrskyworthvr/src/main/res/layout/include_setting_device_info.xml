<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="392dp"
    android:layout_height="368dp"
    android:background="@drawable/shape_setting_normal_bg">

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/tv_setting_main_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="设备信息"
        android:textColor="@color/color_setting_id"
        android:textSize="19sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.131"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.075" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/appCompatTextView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="UUID"
        android:textColor="@color/color_setting_id"
        android:textSize="19sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/tv_setting_main_title"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.37" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/tv_setting_device_info_account_name"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="3dp"
        android:layout_marginEnd="18dp"
        android:textColor="@color/color_setting_id_create"
        android:textSize="12sp"
        app:layout_constraintEnd_toStartOf="@+id/appCompatImageView5"
        app:layout_constraintHorizontal_bias="0"
        app:layout_constraintStart_toStartOf="@+id/appCompatTextView7"
        app:layout_constraintTop_toBottomOf="@+id/tv_setting_device_info_account_id"
        app:layout_constraintWidth_max="wrap"
        tools:text="等待自动生成" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/tv_setting_device_info_account_id"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="18dp"
        android:textColor="@color/color_setting_id_create"
        android:textSize="12sp"
        app:layout_constraintEnd_toStartOf="@+id/appCompatImageView5"
        app:layout_constraintStart_toStartOf="@+id/appCompatTextView7"
        app:layout_constraintTop_toBottomOf="@+id/appCompatTextView7"
        tools:text="392745956839" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/appCompatTextView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ACCOUNT ID"
        android:textColor="@color/color_setting_id"
        android:textSize="19sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/appCompatTextView9"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.652" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/tv_uuid"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="6dp"
        android:layout_marginEnd="10dp"
        android:textColor="@color/color_setting_id_create"
        android:textSize="12sp"
        app:layout_constraintEnd_toStartOf="@+id/appCompatImageView5"
        app:layout_constraintHorizontal_bias="0"
        app:layout_constraintStart_toStartOf="@+id/appCompatTextView9"
        app:layout_constraintTop_toBottomOf="@+id/appCompatTextView9"
        app:layout_constraintWidth_max="wrap"
        tools:text="c92838fa11c92838fa11c92838fa1111" />

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/appCompatImageView5"
        android:layout_width="122dp"
        android:layout_height="264dp"
        android:scaleType="fitXY"
        android:src="@drawable/sky_worth_setting_device_info_icon"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <ProgressBar
        android:id="@+id/pb_device_info_uuid"
        style="?android:attr/progressBarStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="@+id/tv_uuid"
        app:layout_constraintStart_toEndOf="@+id/tv_uuid"
        app:layout_constraintTop_toTopOf="@+id/tv_uuid" />

</androidx.constraintlayout.widget.ConstraintLayout>
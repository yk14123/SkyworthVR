<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="settingType" format="enum">
        <enum name="settingType_wifi" value="1" />
        <enum name="settingType_bluetooth" value="2" />
        <enum name="settingType_alias" value="3" />
    </attr>

    <declare-styleable name="SettingViewGroup">
        <attr name="settingType" />
    </declare-styleable>

    <declare-styleable name="ProgressChart">
        <attr name="progressWidth" format="dimension|reference" />
        <attr name="textSize1" format="dimension|reference" />
        <attr name="textSize2" format="dimension|reference" />
        <attr name="bgColor" format="color|reference" />
        <attr name="progressColor" format="color|reference" />
        <attr name="txtColor" format="color|reference" />
    </declare-styleable>
</resources>